<?xml version="1.0"?>
<robot name="tams_corner">
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="brown">
    <color rgba="0.87 0.72 0.53 1"/>
  </material>
  <material name="lightgray">
    <color rgba="0.6 0.6 0.6 1"/>
  </material>
  <link name="floor">
    <inertial>
      <origin xyz="2.5 2.5 0" />
      <mass value="2000" />
      <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <material name="black" />
      <origin xyz="2.5 2.5 0" />
      <geometry>
        <box size="5.0 5.0 0.001" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="2.5 2.5 0" />
      <geometry>
      <box size="5.0 5.0 0.001" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="floor">
    <material>Gazebo/Blue</material>
    <turnGravityOff>false</turnGravityOff>
    <selfCollide>true</selfCollide>
    <!-- kp: stiffness kd: damping mu1, mu2: friction -->
    <mu1>100.0</mu1>
    <mu2>100.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>
  <link name="wall">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2000" />
      <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <material name="white" />
      <origin xyz="0 0 0" />
      <geometry>
        <box size="0.12 5.0 3.0" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.0" />
      <geometry>
        <box size="0.12 5.0 3.0" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="wall">
    <material>Gazebo/Blue</material>
    <turnGravityOff>false</turnGravityOff>
    <selfCollide>true</selfCollide>
    <!-- kp: stiffness kd: damping mu1, mu2: friction -->
    <mu1>100.0</mu1>
    <mu2>100.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>
  <link name="side_wall">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2000" />
      <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <material name="white" />
      <origin xyz="0 0 0" />
      <geometry>
        <box size="5.0 0.12 3.0" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.0" />
      <geometry>
        <box size="5.0 0.12 3.0" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="side_wall">
    <material>Gazebo/Blue</material>
    <turnGravityOff>false</turnGravityOff>
    <selfCollide>true</selfCollide>
    <!-- kp: stiffness kd: damping mu1, mu2: friction -->
    <mu1>100.0</mu1>
    <mu2>100.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <link name="table">
    <inertial>
      <origin xyz="0 0 0.375" />
      <mass value="10" />
      <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <material name="brown" />
      <origin xyz="0 0 0.375" />
      <geometry>
        <box size="1.4 0.7 0.75" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.375" />
      <geometry>
        <box size="1.4 0.7 0.75" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="table">
    <material>Gazebo/Blue</material>
    <turnGravityOff>false</turnGravityOff>
    <selfCollide>true</selfCollide>
    <!-- kp: stiffness kd: damping mu1, mu2: friction -->
    <mu1>100.0</mu1>
    <mu2>100.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <link name="shelf">
    <inertial>
      <origin xyz="1.7 0 0" />
      <mass value="60" />
      <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <material name="lightgray" />
      <origin xyz="0 1.7 0" />
      <geometry>
        <box size="0.4 2 2.5" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 1.7 0" />
      <geometry>
        <box size="0.4 2 2.5" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="shelf">
    <material>Gazebo/Orange</material>
    <turnGravityOff>false</turnGravityOff>
    <selfCollide>true</selfCollide>
    <!-- kp: stiffness kd: damping mu1, mu2: friction -->
    <mu1>100.0</mu1>
    <mu2>100.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <joint name="floor_wall_joint" type="fixed">
    <parent link="floor"/>
    <child link="wall"/>
    <origin xyz="-0.06 2.5 1.5" rpy="0 0 0" />
  </joint>

  <joint name="side_wall_joint" type="fixed">
    <parent link="floor"/>
    <child link="side_wall"/>
    <origin xyz="2.5 -0.06 1.5" rpy="0 0 0" />
  </joint>

  <joint name="table_joint" type="fixed">
    <parent link="floor"/>
    <child link="table"/>
    <!-- actual wall is 2.5cm further back from table, but is modelled at the position of the small ur5 adapter plate -->
    <origin xyz="0.93 0.24 0" rpy="0 0 0" />
  </joint>

  <joint name="shelf_joint" type="fixed">
    <parent link="floor"/>
    <child link="shelf"/>
    <!-- actual shelf is a little smaller -->
    <origin xyz="0.2 1 1.25" rpy="0 0 0" />
  </joint>


</robot>
